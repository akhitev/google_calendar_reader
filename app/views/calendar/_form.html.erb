<%= form_tag(:controller => "calendar" , :action => "show") do %>
  <%= label_tag(:email, "google email") %>
  <%= text_field_tag(:email) %>
  <%= label_tag(:pass, "password") %>
  <%= password_field_tag (:pass) %>
  <br>
  <%= label_tag(:email, "xml feed") %>
  <%= text_field_tag(:feeds) %>
  <%= submit_tag("Submit") %>
<% end %>